        </activity>
        <activity android:name=".LoginSignup"></activity>
        <activity android:name=".HomeScreen" />
        <activity android:name=".Register" />
        <activity android:name=".Login" />
        <activity android:name=".OnBoarding" />

    </application>

</manifest>